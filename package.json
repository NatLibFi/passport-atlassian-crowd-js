{
	"name": "@natlibfi/passport-atlassian-crowd",
	"description": "Passport strategy for Atlassian Crowd",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/natlibfi/passport-atlassian-crowd-js",
	"bugs": {
		"url": "https://github.com/natlibfi/passport-atlassian-crowd-js/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:natlibfi/passport-atlassian-crowd-js.git"
	},
	"license": "MIT",
	"version": "2.0.3",
	"main": "./dist/index.js",
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=14"
	},
	"scripts": {
		"prepublishOnly": "npm run build",
		"lint": "eslint --fix src",
		"test:base": "cross-env NODE_ENV=test nyc mocha --require @babel/register",
		"test": "npm run lint && npm run test:base -- src/**/*.spec.js && npm run coverage",
		"coverage": "nyc check-coverage --per-file",
		"build": "babel src --source-maps --delete-dir-on-start --out-dir=dist",
		"build:dev": "npm run build && npm run check-deps",
		"watch": "cross-env NODE_ENV=test chokidar src -c 'npm test'"
	},
	"dependencies": {
		"@babel/register": "^7.0.0",
		"http-status": "^1.6.2",
		"moment": "^2.29.4",
		"node-fetch": "^2.6.7",
		"passport-strategy": "^1.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.0",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/preset-env": "^7.20.2",
		"@natlibfi/eslint-config-melinda-backend": "^2.0.2",
		"@natlibfi/fixura": "^2.2.5",
		"babel-plugin-rewire": "^1.2.0",
		"babel-plugin-istanbul": "^6.1.0",
		"chai": "^4.3.7",
		"chai-passport-strategy": "^3.0.0",
		"chokidar-cli": "^3.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.35.0",
		"mocha": "^10.2.0",
		"nock": "^13.3.0",
		"nyc": "^15.1.0"
	},
	"eslintConfig": {
		"parser": "@babel/eslint-parser",
		"extends": "@natlibfi/melinda-backend"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": "maintained node versions"
				}
			]
		],
		"env": {
			"test": {
				"plugins": [
					"rewire",
					"istanbul"
				]
			}
		}
	},
	"nyc": {
		"exclude": [
			"**/*.spec.js"
		],
		"reporter": [
			"lcov",
			"html"
		],
		"require": [
			"@babel/register"
		],
		"sourceMap": false,
		"instrument": false,
		"lines": 80,
		"statements": 80,
		"functions": 80,
		"branches": 80
	}
}
